{
  "name": "mock-iam-v1",
  "version": "1.0.0",
  "description": "API Mocker deployed to Apigee Hosted Targets",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "deploy": "npm i && apigeetool deployhostedtarget -u $APIGEE_USER -p $APIGEE_PASS -o $APIGEE_ORG -e $APIGEE_ENV -n $npm_package_name -b /mock-iam/v1 -v secure -V"
  },
  "author": "LaughingBiscuit",
  "license": "Apache-2.0",
  "dependencies": {
    "apigeetool": "^0.9.2",
    "apimocker": "^1.1.3"
  }
}
